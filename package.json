{
    "name": "css2inlinestyle",
    "displayName": "CSS2InlineStyle",
    "description": "A simple extension to convert CSS to inline style that you can use on your React application.",
    "version": "1.0.0",
    "publisher": "gpanneti",
    "engines": {
        "vscode": "^1.12.0"
    },
    "categories": [
        "Formatters"
    ],
    "activationEvents": [
        "onCommand:extension.css2inlinestyle"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "extension.css2inlinestyle",
            "title": "Convert CSS to inline style"
        }]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}
